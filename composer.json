{
  "name": "bolt/users",
  "description": "Bolt 4 Users Extension",
  "type": "bolt-extension",
  "license": "MIT",
  "authors": [
    {
      "name": "Ivo Valchev",
      "email": "me@ivovalchev.com"
    }
  ],
  "require": {
    "php": "^8.3",
    "twig/twig": "^3.0"
  },
  "require-dev": {
    "bolt/core": "dev-5.2.0-rc.2",
    "symplify/easy-coding-standard": "^12.2",
    "phpstan/phpstan": "^1.11",
    "phpstan/phpstan-symfony": "^1.4"
  },
  "autoload": {
    "psr-4": {
      "Bolt\\UsersExtension\\": "src/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "entrypoint": "Bolt\\UsersExtension\\Extension"
  },
  "config": {
    "allow-plugins": {
      "symfony/flex": true,
      "drupol/composer-packages": true
    }
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "git@github.com:antiftw/bolt-core.git"
    }
  ]
}
